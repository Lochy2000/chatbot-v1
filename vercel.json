{
  "version": 2,
  "functions": {
    "api/chat.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/public/assets/$1"
    },
    {
      "source": "/bots/(.*)",
      "destination": "/public/bots/$1"
    },
    {
      "source": "/icon-gallery.html",
      "destination": "/public/icon-gallery.html"
    }
  ]
}